    "a24-node-query-utils": "git+ssh://git@github.com/A24Group/a24-node-query-utils.git#v1.8.0",
    "a24-node-staffshift-facade-client": "git+ssh://git@github.com/A24Group/a24-node-staffshift-facade-client.git#v1.27.0",
    "arg": "^5.0.1",
    "async": "^3.2.3",
    "async-retry": "^1.3.3",
    "config": "^3.3.6",
    "connect": "3.7.0",
    "http-terminator": "^3.0.0",
    "js-yaml": "3.13.1",
