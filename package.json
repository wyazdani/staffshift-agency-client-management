{
  "name": "staffshift-agency-client-management",
  "version": "1.40.0",
  "description": "Micro service for managing staffshift agency clients",
  "main": "src/http_server.ts",
  "scripts": {
    "build-ts": "tsc",
    "start": "node dist/http_server",
    "start-tracing": "node -r ./tracing.js dist/http_server",
    "test": "node ./testfile-drop-database.js && npm run migrate && npx nyc npx mocha",
    "test-local": "npx tsc --noEmit && TS_NODE_TRANSPILE_ONLY=true npm run test",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore .",
    "migrate": "node_modules/.bin/mongration -f ./migrations -c config/migration-config.js",
    "start-domain-event-consumer": "node dist/ss_domain_event_consumer",
    "start-event-store-publisher": "node dist/event_store_publisher",
    "start-event-store-projection": "node dist/event_store_projection",
    "start-event-store-http-server": "node dist/event_store_http_server",
    "start-bulk-process-manager": "node dist/bulk_process_manager",
    "dev-generate-event-docs": "ts-node src/tools/generate_events_docs.ts",
    "dev": "npm-run-all -l --parallel start start-event-store-publisher start-event-store-http-server start-event-store-projection start-bulk-process-manager start-domain-event-consumer",
    "seed-projection-agency-consultant-roles-v2": "node dist/projections/AgencyConsultantRolesV2/seed_job",
    "seed-projection-agency-clients-v2": "node dist/projections/AgencyClientsV2/seed_job",
    "seed-projection-agency-client-consultants-v3": "node dist/projections/AgencyClientConsultantsV3/seed_job"
  },
  "pre-commit": [
    "lint"
  ],
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "@google-cloud/opentelemetry-cloud-trace-exporter": "1.0.0",
    "@opentelemetry/api": "1.0.3",
    "@opentelemetry/core": "1.0.0",
    "@opentelemetry/instrumentation": "0.26.0",
    "@opentelemetry/instrumentation-http": "0.26.0",
    "@opentelemetry/instrumentation-mongodb": "0.31.0",
    "@opentelemetry/sdk-trace-base": "1.0.0",
    "@opentelemetry/sdk-trace-node": "1.0.0",
    "@opentelemetry/semantic-conventions": "0.19.0",
    "a24-logzio-winston": "git+ssh://git@github.com/A24Group/a24-logzio-winston.git#semver:v3.0.0",
    "a24-node-error-utils": "git+ssh://git@github.com/A24Group/a24-node-error-utils.git#semver:v3.1.1",
