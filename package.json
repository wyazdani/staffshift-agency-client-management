{
  "name": "staffshift-agency-client-management",
  "version": "1.3.0",
  "description": "Micro service for managing staffshift agency clients",
  "main": "src/http_server.ts",
  "scripts": {
    "build-ts": "tsc",
    "start": "node dist/http_server",
    "start-tracing": "node -r ./tracing.js dist/http_server",
    "test": "sh -c 'node ./testfile-drop-database.js' && sh -c 'npm run migrate' && sh -c'node_modules/.bin/nyc node_modules/.bin/mocha'",
    "lint": "./node_modules/.bin/eslint --ignore-path .gitignore . *.js",
    "migrate": "node_modules/.bin/mongration -f ./migrations -c config/migration-config.js",
    "start-domain-event-consumer": "node dist/ss_domain_event_consumer",
    "start-streaming-application-core-agency-client-consultants": "node dist/StreamingApplication --type=core --name=AgencyClientConsultant",
    "start-streaming-application-core-agency-client": "node dist/StreamingApplication --type=core --name=AgencyClientsProjection",
    "start-streaming-application-core-agency-client-event-log": "node dist/StreamingApplication --type=core --name=AgencyClientEventLogProjection"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "@google-cloud/opentelemetry-cloud-trace-exporter": "0.9.0",
    "@opentelemetry/api": "1.0.0-rc.0",
    "@opentelemetry/core": "0.19.0",
    "@opentelemetry/instrumentation": "0.19.0",
    "@opentelemetry/instrumentation-http": "0.19.0",
    "@opentelemetry/instrumentation-mongodb": "0.16.0",
    "@opentelemetry/node": "0.19.0",
    "@opentelemetry/semantic-conventions": "0.19.0",
    "@opentelemetry/tracing": "0.19.0",
    "a24-logzio-winston": "git+ssh://git@github.com/A24Group/a24-logzio-winston.git#v2.0.3",
    "a24-node-error-utils": "git+ssh://git@github.com:A24Group/a24-node-error-utils.git#v2.5.0",
    "a24-node-octophant-utils": "git+ssh://git@github.com:A24Group/a24-node-octophant-utils.git#v1.4.0",
    "a24-node-pubsub": "git+ssh://git@github.com:A24Group/a24-node-pubsub.git#v4.5.0",
    "a24-node-query-utils": "git+ssh://git@github.com:A24Group/a24-node-query-utils.git#v1.8.0",
    "a24-node-staffshift-facade-client": "git@github.com:A24Group/a24-node-staffshift-facade-client.git#v1.27.0",
    "a24-node-timezone-utils": "git+ssh://git@github.com/A24Group/a24-node-timezone-utils.git#v1.8.0",
    "arg": "^5.0.1",
    "async": "3.2.0",
    "config": "^3.3.6",
    "connect": "3.7.0",
    "http-terminator": "^3.0.0",
    "js-yaml": "3.13.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.13.5",
    "mongration": "^1.0.1",
    "swagger-tools": "0.10.4",
    "url": "0.11.0"
  },
  "devDependencies": {
    "chai": "4.3.4",
    "eslint": "^7.32.0",
    "mocha": "9.1.1",
    "nock": "13.1.1",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "swagger-test-templates": "1.6.0",
    "xunit-file": "1.0.0",
    "json-schema-deref": "0.5.0",
    "supertest": "6.1.4",
    "a24nodetestutils": "git+ssh://git@github.com:A24Group/A24NodeTestUtils.git#v2.2.0",
    "rewire": "5.0.0",
    "z-schema": "4.2.4",
    "superagent": "6.1.0",
    "nyc": "15.1.0",
    "mock-require": "3.0.3",
    "ts-node": "10.2.1",
    "typescript": "4.4.2",
    "source-map-support": "0.5.20",
    "@typescript-eslint/eslint-plugin": "4.31.0",
    "@typescript-eslint/parser": "4.31.0",
    "@types/jsonwebtoken": "8.5.1",
    "@types/sinon": "10.0.2",
    "@types/chai": "4.2.21",
    "@types/mocha": "9.0.0",
    "@types/config": "0.0.39",
    "@types/supertest": "2.0.11"
  }
}
