    "@opentelemetry/node": "0.19.0",
    "@opentelemetry/semantic-conventions": "0.19.0",
    "@opentelemetry/tracing": "0.19.0",
    "a24-logzio-winston": "git+ssh://git@github.com/A24Group/a24-logzio-winston.git#v2.0.3",
    "a24-node-error-utils": "git+ssh://git@github.com:A24Group/a24-node-error-utils.git#v2.5.0",
