    "@types/http-terminator": "^2.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mocha": "9.0.0",
    "@types/sinon": "10.0.2",
    "@types/sinon-chai": "^3.2.5",
    "@types/supertest": "2.0.11",
    "@types/swagger-tools": "^0.10.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.0",
