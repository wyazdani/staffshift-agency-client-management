    "@types/config": "0.0.39",
    "@types/connect": "^3.4.35",
    "@types/flat": "5.0.2",
    "@types/http-terminator": "^2.0.2",
    "@types/js-yaml": "^4.0.3",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/lodash": "^4.14.178",
    "@types/mocha": "9.0.0",
